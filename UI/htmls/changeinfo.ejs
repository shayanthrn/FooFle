    <main class="profile-user-page default" style="position: absolute;
                                                    top:100px;
                                                    right: 203px;">
        <div class="container">
            <div class="row">
                <div class="profile-page col-xl-9 col-lg-8 col-md-12 order-2">
                    <div class="row">
                        <div class="col-12">
                            <div class="col-12">
                                <h1 class="title-tab-content" style="text-align: left;">Signup </h1>
                            </div>
                            <div class="content-section default">
                                
                                <form class="form-account" style="text-align: left;" method="POST">
                                    <div class="row">
                                        <div class="col-sm-12 col-md-6">
                                            <div class="form-account-title" style="text-align: left;">password</div>
                                            <div class="form-account-row">
                                                <input class="input-field text-right" name="password" type="password">
                                            </div>
                                        </div>
                                        <div class="col-sm-12 col-md-6">
                                            <div class="form-account-title">phonenumber</div>
                                            <div class="form-account-row">
                                                <input class="input-field"  name="phonenumber" type="text" >
                                            </div>
                                        </div>
                                        <div class="col-sm-12 col-md-6">
                                            <div class="form-account-title">account phonenumber</div>
                                            <div class="form-account-row">
                                                <input class="input-field" name="accphonenumber" type="text">
                                            </div>
                                        </div>
                                        <div class="col-sm-12 col-md-6">
                                            <div class="form-account-title">first name </div>
                                            <div class="form-account-row">
                                                <input class="input-field" name="firstname" type="text" >
                                            </div>
                                        </div>
                                        <div class="col-sm-12 col-md-6">
                                            <div class="form-account-title">last name</div>
                                            <div class="form-account-row">
                                                <input class="input-field" name="lastname" type="text" >
                                            </div>
                                        </div>
                                        <div class="col-sm-12 col-md-6">
                                            <div class="form-account-title">birth date </div>
                                            <div class="form-account-row">
                                                <input class="input-field" name="birthdate" type="text" >
                                            </div>
                                        </div>
                                        <div class="col-sm-12 col-md-6">
                                            <div class="form-account-title">nickname</div>
                                            <div class="form-account-row">
                                                <input class="input-field" name="nickname" type="text" >
                                            </div>
                                        </div>
                                        <div class="col-sm-12 col-md-6">
                                            <div class="form-account-title"> id number </div>
                                            <div class="form-account-row">
                                                <input class="input-field" name="idnum" type="text" >
                                            </div>
                                        </div>
                                        <div class="col-sm-12 col-md-6">
                                            <div class="form-account-title">address</div>
                                            <div class="form-account-row">
                                                <input class="input-field" name="address" type="text" >
                                            </div>
                                        </div>
                                        <div class="col-sm-12 col-md-6">
                                        </div>
                                        <div class="col-sm-12 col-md-6">
                                            <select name="accessibility">
                                                <option value="1">yes</option>
                                                <option value="0">no</option>
                                            </select>
                                            ?can others see your info
                                        </div>
                                    </div>
                                    <div class="col-12 text-center">
                                        <button class="btn btn-default btn-lg" type="submit" >submit</button>
                                        <a class="btn btn-default btn-lg" id="123" href="/">cancel</a>
                                    </div>
                                </form>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
